{
  "name": "@rivetkit/svelte",
  "version": "0.1.0",
  "description": "Svelte 5 runes integration for RivetKit actors",
  "license": "Apache-2.0",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
