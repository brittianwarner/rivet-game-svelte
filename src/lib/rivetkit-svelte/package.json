  "devDependencies": {
    "@sveltejs/package": "^2.5.7",
    "svelte": "^5.0.0",
    "svelte-check": "^4.4.3",
    "typescript": "^5.5.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "rivetkit",
    "svelte",
    "svelte5",
    "runes",
    "actors",
    "realtime",
    "websocket"
